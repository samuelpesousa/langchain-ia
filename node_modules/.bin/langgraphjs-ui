#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules/@langchain/langgraph-ui/dist/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules/@langchain/langgraph-ui/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules/@langchain/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules/@langchain/langgraph-ui/dist/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules/@langchain/langgraph-ui/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules/@langchain/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/@langchain+langgraph-ui@1.1.12/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@langchain/langgraph-ui/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../@langchain/langgraph-ui/dist/cli.mjs" "$@"
fi
