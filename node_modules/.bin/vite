#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.33_jiti@2.6.1_lightningcss@1.30.2_tsx@4.21.0_yaml@2.8.2/node_modules/vite/bin/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.33_jiti@2.6.1_lightningcss@1.30.2_tsx@4.21.0_yaml@2.8.2/node_modules/vite/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.33_jiti@2.6.1_lightningcss@1.30.2_tsx@4.21.0_yaml@2.8.2/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.33_jiti@2.6.1_lightningcss@1.30.2_tsx@4.21.0_yaml@2.8.2/node_modules/vite/bin/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.33_jiti@2.6.1_lightningcss@1.30.2_tsx@4.21.0_yaml@2.8.2/node_modules/vite/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/vite@7.3.1_@types+node@20.19.33_jiti@2.6.1_lightningcss@1.30.2_tsx@4.21.0_yaml@2.8.2/node_modules:/home/samuel-sousa/Documentos/langchain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
